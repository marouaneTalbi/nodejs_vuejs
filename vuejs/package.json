    "@vue/tsconfig": "^0.4.0",
    "cypress": "^12.12.0",
    "jsdom": "^22.0.0",
    "node-sass": "^9.0.0",
    "npm-run-all": "^4.1.5",
    "sass-loader": "^13.3.2",
    "start-server-and-test": "^2.0.0",
    "typescript": "~5.0.4",
    "vite": "^4.3.5",
    "vitest": "^0.31.0",
    "vue-tsc": "^1.6.4"
  }
